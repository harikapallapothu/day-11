            <input type="text"  name = "address"></br></br>
            <label for = "city">City</label>
            <input type="text" name = "city">&nbsp;&nbsp;
            <label for= "state">State: </label>
            <input type="text" name = "state">&nbsp;&nbsp;
            <label for="pincode">Zip: </label>
            <input type ="text" name = "pincode"></br>
            <div id = subscribe></br>
                Subscribe for:
                
                    <input type="checkbox" name = "1years">
                    <label for = "1years">1 Year</label>
                    <input type="checkbox" name = "2years">
                    <label for = "2years">2 Year</label>
            </div>
            <div id = "information"></br>
                Send me more information about:
                <div id = "checkbox2">
                    <input type="checkbox" name = "computer_Zine">
                    <label for = "computer_Zine">Computer_Zine</label></br>
                    <input type="checkbox" name = "fishing_Zine">
                    <label for = "fishing_Zine">Fishing_Zine</label></br>
                    <input type="checkbox" name = "cat_O_Zine">
                    <label for = "cat_O_Zine">Cat O Zine</label></br>
                </div>

            </div>

    </div>

</body>

</html>
body{
    border: 5px solid black;
    margin-left: 100px;
    margin-top: 100px;
    margin-right: 100px;
    margin-bottom: 100px;
       
}
#title{
    background-color: red;
    color: white;
    font-size: 20px;
    margin-left: 120px;
    margin-right: 120px;
    text-align-last: center; 
    font-weight: bold;
}
#information{
    font-weight: bold;
    font-family: Arial, Helvetica, sans-serif;
}
#subscribe{
    font-family: Arial, Helvetica, sans-serif;
    font-weight: bold;
}
#form{
    margin-left: 20px;
    margin-bottom: 10px;
}
